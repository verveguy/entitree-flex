{
  "printWidth": 120,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "arrowParens": "always",
  "overrides": [
    {
      "files": "*.ts,*.tsx",
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
